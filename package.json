{
  "name": "project-starter",
  "version": "1.0.0",
  "description": "Template moderne pour sites statiques avec Tailwind CSS v4.1",
  "main": "vite.config.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "postbuild": "node scripts/post-build.js",
    "preview": "vite preview",
    "lint": "eslint . --ext .js",
    "format": "prettier --write \"**/*.{js,html,css}\"",
    "clean": "rimraf dist",
    "deploy": "npm run build && node scripts/deploy.js",
    "fix-css": "node scripts/fix-build.js",
    "build-fix": "npm run build && npm run fix-css",
    "test": "jest",
    "test:watch": "jest --watch",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "security:outdated": "npm outdated",
    "preinstall": "npx npm-force-resolutions",
    "setup-tailwind": "npm install -D tailwindcss@^4.1 @tailwindcss/postcss@^4.1 @tailwindcss/vite@^4.1 @tailwindcss/forms @tailwindcss/typography"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@tailwindcss/forms": "0.5.10",
    "@tailwindcss/postcss": "4.1.4",
    "@tailwindcss/typography": "0.5.16",
    "@tailwindcss/vite": "4.1.4",
    "autoprefixer": "10.4.21",
    "basic-ftp": "5.0.5",
    "browserslist": "4.24.4",
    "caniuse-lite": "1.0.30001714",
    "dotenv": "16.5.0",
    "electron-to-chromium": "1.5.138",
    "esbuild": "0.25.2",
    "escalade": "3.2.0",
    "eslint": "9.24.0",
    "fraction.js": "5.2.2",
    "fsevents": "2.3.3",
    "jest": "^29.7.0",
    "nanoid": "5.1.5",
    "node-releases": "2.0.19",
    "normalize-range": "0.1.2",
    "picocolors": "1.1.1",
    "postcss": "^8.5.3",
    "postcss-value-parser": "4.2.0",
    "prettier": "^3.5.3",
    "rollup": "4.40.0",
    "source-map-js": "1.2.1",
    "tailwindcss": "4.1.4",
    "update-browserslist-db": "1.1.3",
    "vite": "^6.3.2"
  },
  "dependencies": {
    "@alpinejs/intersect": "3.14.9",
    "alpinejs": "3.14.9",
    "aos": "2.3.4",
    "lodash": "^4.17.21"
  },
  "resolutions": {
    "minimist": ">=1.2.6",
    "glob-parent": ">=5.1.2"
  }
}
